<?xml version="1.0" encoding="UTF-8"?>
<PhoneColors>
        <!--PhoneColor.name：手机颜色的唯一标识，由项目、手软提供-->
        <!--ColorDescription：手机颜色描述，描述手机颜色-->
        <!--ColorDescriptionCN：手机颜色描述（中文），描述手机颜色-->
        <!--DefaultTheme：该颜色手机对应的主题，可选为default（对应/system/media/theme/default/下默认主题）、/system/media/themeInner/下内置主题的文件名，不包含文件后缀（如.theme）-->
        <!--ThemeDescriptionCN：主题描述（中文），描述主题名称-->
        <!--手机颜色唯一标识、手机颜色描述由项目或手软提供，对应主题由桌面主题组根据UI要求配置-->
        <!--该文件由OS产品中心桌面主题组维护-->

    <PhoneColor name="FF190284">
        <ColorDescription>Aurora Blue</ColorDescription>
        <ColorDescriptionCN>星云紫</ColorDescriptionCN>
        <DefaultTheme>1-AfflatusFromNebula</DefaultTheme>
        <ThemeDescriptionCN>星云灵感</ThemeDescriptionCN>
    </PhoneColor>

    <PhoneColor name="FF005355">
        <ColorDescription>Jade Black</ColorDescription>
        <ColorDescriptionCN>秘境黑</ColorDescriptionCN>
        <DefaultTheme>2-BlackishGreen</DefaultTheme>
        <ThemeDescriptionCN>墨绿溢彩</ThemeDescriptionCN>
    </PhoneColor>

    <PhoneColor name="FFDBA8DE">
        <ColorDescription>Pearl White</ColorDescription>
        <ColorDescriptionCN>晨曦白</ColorDescriptionCN>
        <DefaultTheme>3-FlowingPink</DefaultTheme>
        <ThemeDescriptionCN>蔻粉流光</ThemeDescriptionCN>
    </PhoneColor>
</PhoneColors>
